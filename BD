
drop table  trophee;
drop table representants;

-- create tables --

create table trophee(
award  varchar2(5),
dateA varchar(10),
vainqueur varchar2(100),
categorie varchar2(100),
nomines  varchar2(600),
audience varchar2(15) , 
artistesPresents varchar2(600),
remiseTrophee varchar2(100),
performances varchar2(1000),
partdemarche varchar2(100),
sponsors varchar2(150),
lieu varchar2(50),
diffusion varchar2(150),
production varchar2(100),
realisation varchar2(100),
presentateur  varchar2(50)
);

create table representants(
	artiste varchar2(400),
	pays varchar2(150),
	continent varchar2(50)
);



-- insertion de donnees de la table trophee

