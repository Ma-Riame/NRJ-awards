drop table representants;
drop table  trophee;

-- create tables

create table representants(
 award  varchar2(5) not null,
 pays   varchar2(50) not null,
 artiste varchar2(50) not null,
  constraint pk_represent primary key (award,pays)
);

create table trophee(
award  varchar2(5) not null,
vainqueur varchar2(50) not null,
categorie varchar2(100) not null,
nomines  varchar2(400) not null,
presentateur  varchar2(50) not null,
constraint pk_trophee primary key (award,vainqueur,categorie)
);

-- insertion de donnees de la table trophee

